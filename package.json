{
  "name": "jenkins-pipelines-test",
  "version": "1.0.0",
  "description": "CI test with Node.js",
  "main": "index.js",
  "scripts": {
    "start": "nodemon src/app.js --exec babel-node --presets es2015,stage-2",
    "build": "babel src -d dist",
    "serve": "node dist/app.js",
    "test": "mocha tests --recursive --compilers js:babel-register",
    "test-jenkins": "jenkins-mocha test/* --compilers js:babel-register"
  },
  "author": "dobrakmato",
  "license": "ISC",
  "dependencies": {
    "@types/chai": "^4.0.1",
    "@types/express": "^4.0.36",
    "@types/mocha": "^2.2.41",
    "express": "^4.15.3",
    "intersect": "^1.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.24.1",
    "chai": "^4.1.0",
    "jenkins-mocha": "^5.0.0",
    "mocha": "^3.4.2",
    "nodemon": "^1.11.0"
  }
}
